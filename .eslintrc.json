{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js"]
            }        
        }
    }
}
